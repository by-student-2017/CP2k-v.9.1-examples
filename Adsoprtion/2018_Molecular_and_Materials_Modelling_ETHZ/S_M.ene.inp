&FORCE_EVAL
  METHOD Quickstep
  &DFT
    #CHARGE 1
    #LSD
    #MULTIPLICITY 2
  &PRINT
    &E_DENSITY_CUBE
    &END E_DENSITY_CUBE
  &END
    BASIS_SET_FILE_NAME BASIS_MOLOPT
    POTENTIAL_FILE_NAME GTH_POTENTIALS
    #POTENTIAL_FILE_NAME GTH_SOC_POTENTIALS
    &QS
      EPS_DEFAULT 1.0E-10
      METHOD GPW
      EXTRAPOLATION ASPC
      EXTRAPOLATION_ORDER 3
    &END QS
    &MGRID
      CUTOFF 400
      NGRIDS 5
    &END
    &SCF
      MAX_SCF 20
      SCF_GUESS RESTART
      EPS_SCF 1.0E-5
      &OT
        PRECONDITIONER  FULL_SINGLE_INVERSE
        MINIMIZER  CG
      &END
      &OUTER_SCF
        MAX_SCF 50
        EPS_SCF 1.0E-5
      &END
      &PRINT
      &END
    &END SCF
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
    &POISSON
       PERIODIC XY
       POISSON_SOLVER ANALYTIC
    &END
  &END DFT
  &SUBSYS
    &CELL
A [angstrom] 7.042785 0 0
B [angstrom] 0  6.09925  0.
C [angstrom] 0.000000   0.   15.
       PERIODIC XY
    &END CELL
    &TOPOLOGY
     COORD_FILE_NAME ./S_M.opt.xyz
     #COORD_FILE_NAME ./xyz/C2H6_1L-PdGa_TiO2.xyz
     #COORD_FILE_NAME ./xyz/C2H6_2L-PdGa_TiO2.xyz
     COORDINATE xyz
    &END
    &KIND O
      BASIS_SET DZVP-MOLOPT-SR-GTH
      POTENTIAL GTH-PBE-q6
    &END KIND
    &KIND Ti
      BASIS_SET DZVP-MOLOPT-SR-GTH
      POTENTIAL GTH-PBE-q12
    &END KIND
    &KIND Pd
      BASIS_SET DZVP-MOLOPT-SR-GTH-q18
      POTENTIAL GTH-PBE-q18
    &END KIND
    &KIND Ga
      BASIS_SET DZVP-MOLOPT-SR-GTH-q13
      POTENTIAL GTH-PBE-q13
    &END KIND
    &KIND C
      BASIS_SET TZV2P-MOLOPT-GTH
      POTENTIAL GTH-PBE-q4
    &END KIND
    &KIND H
      BASIS_SET TZV2P-MOLOPT-GTH
      POTENTIAL GTH-PBE-q1
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PRINT_LEVEL LOW
  PROJECT S_M
  #RUN_TYPE ENERGY # ENERGY or MD
  RUN_TYPE MD
  WALLTIME 860000
&END GLOBAL
&END